<script type="text/javascript">

    $(document).on("wheel", "input[type=number]", function (e) {
        $(this).blur();
    });

    $('.slider').slick({
        rtl: true,
        autoplay: true,
        autoplaySpeed: 3000,
        dots: true,
        arrows: false,

    });

    $("#subs").submit(function(e) {

        var url = "https://api.travil.io/node/subscribe"; // the script where you handle the form input.
        var memail = $("#email").val();
        $('#subs .btn-success').attr("disabled", "disabled");
        $.ajax({
            method: 'POST',
            url: url,
            data: {email: memail}
        }).done(
            function (msg) {
                $('#subs').hide('fast');
                $('.ok').show('slow');
            }
        ).fail(function (status, text) {
            $('#subs .btn-success').removeAttr("disabled");
            $('.notok').slideDown('slow');

        });
        e.preventDefault(); // avoid to execute the actual submit of the form.
    });


    $("#mailsubs").submit(function(e) {

        var url = "https://api.travil.io/node/subscribe"; // the script where you handle the form input.
        //var url = "http://localhost:3000/subscribe"; // the script where you handle the form input.
        var memail = $("#email").val();
        var memobile = $("#mobile").val();
        $('#subs .btn-success').attr("disabled", "disabled");
        $.ajax({
            method: 'POST',
            url: url,
            data: {email: memail, mobile: memobile}
        }).done(
            function (msg) {
                $('#mailsubs').hide('fast');
                $('.notok').hide('fast');
                $('.ok').show('slow');
            }
        ).fail(function (status, text) {
            $('#subs .btn-success').removeAttr("disabled");
            $('.notok').slideDown('slow');

        });
        e.preventDefault(); // avoid to execute the actual submit of the form.
    });


